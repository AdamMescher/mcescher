{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "prettier",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "react/display-name": [
            0
        ],
        "react/react-in-jsx-scope": [
            0
        ]
    }
}